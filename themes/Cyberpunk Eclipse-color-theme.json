{
	"name": "Cyberpunk Eclipse",
	"type": "dark",
	"colors": {
	  "editor.background": "#0f0f1a",
	  "editor.foreground": "#ff46a2",
	  "editorCursor.foreground": "#00ffe7",
	  "editor.lineHighlightBackground": "#2a2a40",
	  "editor.selectionBackground": "#1e1e2f",
	  "editorLineNumber.foreground": "#5c5c8a",
	  "editorLineNumber.activeForeground": "#ffee80"
	},
	"tokenColors": [
	  {
		"scope": ["keyword"],
		"settings": {
		  "foreground": "#ff2c70",
		  "fontStyle": "bold"
		}
	  },
	  {
		"scope": ["variable", "meta.definition.variable.name"],
		"settings": {
		  "foreground": "#f92672"
		}
	  },
	  {
		"scope": ["entity.name.function", "support.function"],
		"settings": {
		  "foreground": "#00ffe7"
		}
	  },
	  {
		"scope": ["string"],
		"settings": {
		  "foreground": "#ffee80"
		}
	  },
	  {
		"scope": ["constant.numeric"],
		"settings": {
		  "foreground": "#ff6ac1"
		}
	  },
	  {
		"scope": ["constant.language.boolean"],
		"settings": {
		  "foreground": "#00f7ff"
		}
	  },
	  {
		"scope": ["comment"],
		"settings": {
		  "foreground": "#7f8c8d",
		  "fontStyle": "italic"
		}
	  },
	  {
		"scope": ["keyword.operator", "punctuation"],
		"settings": {
		  "foreground": "#ff8b39"
		}
	  },
	  {
		"scope": ["invalid"],
		"settings": {
		  "foreground": "#ffffff"
		}
	  }
	]
  }
  